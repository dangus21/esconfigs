{
	"name": "@angusmiguel/esconfigs",
	"version": "8.2.1",
	"description": "Generate linting configs for js",
	"bugs": {
		"url": "https://github.com/dangus21/esconfigs/issues"
	},
	"homepage": "https://github.com/dangus21/esconfigs#readme",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.cjs",
	"types": "./dist/types.d.ts",
	"files": [
		"./dist"
	],
	"bin": {
		"esconfigs": "./dist/index.cjs"
	},
	"scripts": {
		"lint": "eslint --fix src",
		"pretty": "prettier src --write",
		"format": "pnpm pretty && pnpm lint",
		"test_build": "node ./dist/index.js",
		"build": "rimraf dist && tsup",
		"pub": "pnpm publish --access public && git push -f",
		"pp": "pnpm version patch --force && pnpm publish --access public && git push -f",
		"pm": "pnpm version minor --force && pnpm publish --access public && git push -f",
		"pmm": "pnpm version major --force && pnpm publish --access public && git push -f",
		"test": "tsx ./src/index.ts",
		"del_test": "rimraf test_env/node_modules test_env/pnpm-lock.yaml test_env/.eslintrc.js test_env/.prettierrc test_env/.editorConfig test_env/biome.json",
		"del_curr": "rimraf ./"
	},
	"keywords": [
		"lint",
		"format",
		"javascript",
		"ecmascript",
		"prettier",
		"biomejs",
		"config",
		"editorConfig",
		"tooling"
	],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@next/eslint-plugin-next": "^14.2.13",
		"@swc/core": "^1.7.28",
		"@types/node": "^20.16.9",
		"@types/prompts": "^2.4.9",
		"@typescript-eslint/eslint-plugin": "^8.7.0",
		"@typescript-eslint/parser": "^8.7.0",
		"acorn": "^8.12.1",
		"esbuild": "^0.19.12",
		"eslint": "^8.57.1",
		"eslint-import-resolver-typescript": "^3.6.3",
		"eslint-plugin-babel": "^5.3.1",
		"eslint-plugin-html": "^8.1.2",
		"eslint-plugin-import": "^2.30.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-react": "^7.36.1",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
		"eslint-plugin-unused-imports": "^4.1.4",
		"prettier": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"prompts": "^2.4.2",
		"react": "^18.3.1",
		"rimraf": "^5.0.10",
		"ts-node": "^10.9.2",
		"tslib": "^2.7.0",
		"tsup": "^8.3.0",
		"tsx": "^4.19.1",
		"typescript": "^5.6.2"
	},
	"engines": {
		"node": "^18.18.0 || ^20.9.0 || >=21.1.0"
	}
}
